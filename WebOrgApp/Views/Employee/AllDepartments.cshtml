@model List<string>
@{
    ViewData["Title"] = "View all departments";
}

<h2>View all departments</h2>

<a asp-controller="Employee" asp-action="Index" asp-route-id="">Back</a>
<br />

<table class="table table-striped">
    <tr>
        <th>Department name</th>
    </tr>

        @foreach (var department in Model)
        {
            <tr>
                <td>@department</td>
            </tr>
        }
</table>